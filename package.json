{
  "name": "@foxfriends/embedded-typescript",
  "version": "0.4.0",
  "description": "Type safe TypeScript templates",
  "license": "MIT",
  "author": "Cameron Eldridge <cameldridge@gmail.com>",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ets": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "e2e:setup": "npm run package:build && (cd e2e && npm install && npm run build)",
    "fmt": "prettier --write .",
    "lint": "eslint src --max-warnings 0",
    "prepare": "tsc --build",
    "prepublish": "tsc --build --force",
    "test": "jest",
    "test:ci": "test --coverage",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foxfriends/embedded-typescript"
  },
  "dependencies": {
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "embedded typescript",
    "ets",
    "type safe string",
    "type safe template",
    "type safe templating",
    "typescript template",
    "typescript templating"
  ],
  "engines": {
    "node": "^18 || ^20",
    "npm": "^9 || ^10"
  },
  "files": [
    "dist",
    "src"
  ]
}
